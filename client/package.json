{
  "name": "ai-call-center-client",
  "version": "1.0.0",
  "description": "WebSocket client for AI call center communication",
  "main": "dist/app/main.js",
  "scripts": {
    "build": "tsc",
    "bundle": "node build-bundle.js",
    "build:all": "npm run build && npm run bundle",
    "dev": "tsc --watch",
    "test": "jest",
    "clean": "rm -rf dist client-bundle.js client-bundle.js.map",
    "serve": "node serve.js",
    "start": "npm run build:all && npm run serve"
  },
  "keywords": [
    "websocket",
    "typescript",
    "client",
    "communication"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "esbuild": "^0.25.10",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  }
}
