{
  "name": "ai-call-center",
  "version": "1.0.0",
  "description": "AI Call Center with OpenAI Realtime API and Twilio",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build:client": "tsc -p src/presentation/public/tsconfig.json",
    "start": "npm run build:client && tsx src/index.ts",
    "dev": "npm run build:client && tsx watch src/index.ts",
    "dev:client": "tsc -p src/presentation/public/tsconfig.json --watch"
  },
  "keywords": [
    "openai",
    "twilio",
    "realtime",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/formbody": "^8.0.1",
    "@fastify/static": "^8.2.0",
    "@fastify/websocket": "^11.0.1",
    "@openai/agents": "0.0.1",
    "@openai/agents-extensions": "0.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
